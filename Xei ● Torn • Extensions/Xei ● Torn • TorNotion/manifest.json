{
    "manifest_version": 3,
    "name": "TorNotion",
    "version": "1.0",
    "permissions": ["storage"],
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [
      {
        "matches": ["https://www.torn.com/*"],
        "js": ["jquery.min.js", "content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
