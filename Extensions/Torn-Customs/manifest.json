{
  "manifest_version": 3,
  "name": "Xei ● Torn • Customs",
  "version": "0.5",
  "description": "Customizes Torn.com",
  "author": "XeiDaMoKa [2373510]",


  "icons": {
    "16": "icons/xeitorncustomsicon16x16.png",
    "32": "icons/xeitorncustomsicon32x32.png",
    "48": "icons/xeitorncustomsicon48x48.png",
    "128": "icons/xeitorncustomsicon128x128.png"
  },

  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "downloads"
],

  "action": {
    "default_icon": {
    "16": "icons/xeitorncustomsicon16x16.png",
    "32": "icons/xeitorncustomsicon32x32.png",
    "48": "icons/xeitorncustomsicon48x48.png",
    "128": "icons/xeitorncustomsicon128x128.png"
    },
    "default_title": "Torn ● Customs • Settings",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://www.torn.com/*"],
      "js": ["jquery.js" , "customs.js"]
    }
  ]
}