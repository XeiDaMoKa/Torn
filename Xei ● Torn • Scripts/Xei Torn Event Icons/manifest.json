{
    "name": "Xei ● Torn • Event Icons",
    "description": "Icon SCGs in Events Page",
    "version": "0.0.1",
    "author": "XeiDaMoKa [2373510]",
    "manifest_version": 3,

    "icons": {
        "16": "Icon/icon16.png",
        "32": "Icon/icon32.png",
        "48": "Icon/icon48.png",
        "128": "Icon/icon128.png"
  },

    "action": {
        "default_title": "Torn ● Event Icons • Settings",
        "default_popup": "/popup.html"
  },

  "web_accessible_resources": [
    {
      "resources": ["Images/*"],
      "matches": ["*://*.torn.com/*"]
    }
  ],
    "content_scripts": [
      {
        "matches": ["*://*.torn.com/*"],
        "js": [
        "jquery.js",
        "Script/script.js"
      ]
    }
  ]
}
