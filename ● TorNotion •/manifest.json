{
    "manifest_version": 3,
    "name": "TorNotion",
    "version": "1.0",
    "description": "Description of your extension",
    "icons": {
        "16": "icons/tornotionicon16x16.png",
        "32": "icons/tornotionicon32x32.png",
        "48": "icons/tornotionicon48x48.png",
        "128": "icons/tornotionicon128x128.png"
      },
      "permissions": [
        "storage",
        "activeTab"
      ],
      "host_permissions": [
        "https://*.notion.so/"
      ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
        "16": "icons/tornotionicon16x16.png",
        "32": "icons/tornotionicon32x32.png",
        "48": "icons/tornotionicon48x48.png",
        "128": "icons/tornotionicon128x128.png"
        },
        "default_title": "Torn ● Customs • Settings",
        "default_popup": "popup.html"
      }
  }
